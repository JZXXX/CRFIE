{
    "bert_model_name": "bert-large-cased",
    "bert_cache_dir": "bert/",
    "multi_piece_strategy": "average",
    "bert_dropout": 0.5,
    "use_extra_bert": true,
    "extra_bert": -3,

    "use_global_features": false,
    "global_features": [],
    "global_warmup": 0,

    "use_high_order": false,
    "use_high_order_tre": false,
    "use_high_order_sibling": false,
    "use_high_order_coparent": false,
    "use_high_order_ere": false,
    "use_high_order_er": false,
    "new_score": true, 
    "decomp_size": 300,
    "mfvi_iter": 1,
    "rebatch": false,
    "trigger_maxent": false,
    "split_train": true,
    "ident_model_path": "log/dygie/ident/final.ident.mdl",

    "linear_dropout": 0.4,
    "linear_bias": true,
    "entity_hidden_num": 150,
    "mention_hidden_num": 150,
    "event_hidden_num": 600,
    "relation_hidden_num": 150,
    "role_hidden_num": 600,
    "use_entity_type": true,
    "beam_size": 20,
    "beta_v": 2,
    "beta_e": 2,
    "relation_mask_self": true,
    "relation_directional": true,
    "symmetric_relations": ["PER-SOC"],



    "train_file": "data/dygie/train.oneie.json",
    "dev_file": "data/dygie/dev.oneie.json",
    "test_file": "data/dygie/test.oneie.json",
    "log_path": "log/dygie/",
    "valid_pattern_path": "resource/valid_patterns",
    "ignore_title": false,
    "ignore_first_header": false,

    "accumulate_step": 1,
    "batch_size": 10,
    "eval_batch_size": 10,
    "max_epoch": 80,
    "learning_rate": 1e-3,
    "bert_learning_rate": 1e-5,
    "weight_decay": 1e-3,
    "bert_weight_decay": 1e-5,
    "warmup_epoch": 5,
    "grad_clipping": 5.0,

    "use_gpu": true,
    "gpu_device": 0
  }